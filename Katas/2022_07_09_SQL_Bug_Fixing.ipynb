   "source": [
    "SELECT \n",
    "  DATE(s.transaction_date) as \"day\",\n",
    "  d.name AS department,\n",
    "  COUNT(s.id) AS sale_count\n",
    "  FROM department d\n",
    "    INNER JOIN sale s on d.id = s.department_id\n",
    "  group by \"day\", department\n",
    "  order by \"day\""
   ]
  }
