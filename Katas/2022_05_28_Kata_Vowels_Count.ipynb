   "source": [
    "def get_count(sentence):\n",
    "    i = 0\n",
    "    for x in sentence:\n",
    "        if x == 'a' or x == 'e' or x == 'i' or x == 'o' or x == 'u':\n",
    "            i = i + 1  \n",
    "    return i"
   ]
